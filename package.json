    "os-browserify": "0.3.0",
    "process": "0.11.10",
    "source-map-loader": "5.0.0",
    "ts-loader": "9.5.4",
    "typescript": "5.9.2",
    "webpack": "5.101.3",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.2",
