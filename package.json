  "dependencies": {
    "core-js": "3.45.1",
    "regenerator-runtime": "0.14.1"
  },
  "devDependencies": {
    "@babel/core": "7.28.3",
    "@babel/preset-typescript": "7.27.1",
    "@types/office-js": "1.0.529",
